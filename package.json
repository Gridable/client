{
  "name": "scaffold-react",
  "version": "1.0.0",
  "description": "Creating scaffold template for react app",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "analyze": "webpack --env mode=production analyze=true",
    "lint:ts": "eslint \"src/**/*.{ts,tsx,js,jsx}\" -c ./config/eslint/.eslintrc.json",
    "lint:ts:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" -c ./config/eslint/.eslintrc.json --fix",
    "lint:scss": "stylelint src/**/*.scss -c ./config/stylelint/.stylelintrc.json",
    "lint:scss:fix": "stylelint src/**/*.scss -c ./config/stylelint/.stylelintrc.json --fix",
    "format": "prettier --write src !**/*.{scss,css,json}",
    "prepare": "husky install",
    "test:unit": "jest --config ./config/jest/jest.config.ts --passWithNoTests",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "build-storybook": "storybook build -c ./config/storybook"
  },
  "keywords": [],
  "author": "Karagoishin Zhaslan",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@storybook/addon-essentials": "^7.1.1",
    "@storybook/addon-interactions": "^7.1.1",
    "@storybook/addon-links": "^7.1.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.1.1",
    "@storybook/react": "^7.1.1",
    "@storybook/react-webpack5": "^7.1.1",
    "@storybook/testing-library": "^0.2.0",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.18.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "eslint": "^8.40.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.13",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.8",
    "react-refresh": "^0.14.0",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "storybook": "^7.1.1",
    "style-loader": "^3.3.1",
    "stylelint": "^15.6.1",
    "stylelint-config-standard-scss": "^9.0.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "axios": "^1.4.0",
